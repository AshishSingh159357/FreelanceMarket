arget.value;\r\n      this.setState({ [name]: value });\r\n      this.setState({UsernameError:\"\"});\r\n      this.setState({PasswordError:\"\"})\r\n   }\r\n\r\n   render() {\r\n      if (this.state.login) {\r\n         return <Redirect to=\"/dashboard\" />\r\n      }\r\n      else {\r\n         return (\r\n            <div className=\"Main\">\r\n\r\n               <div className=\"container\">\r\n                  <div className=\"login\">\r\n                     <h2>Login</h2>\r\n                     <form>\r\n                        <input className=\"username-input\" type=\"text\" name=\"username\" placeholder=\"Username\" onChange={(e) => this.changeInput(e)} />\r\n                        <input className=\"password-input\" type=\"password\" name=\"password\" placeholder=\"Password\" onChange={(e) => this.changeInput(e)} />\r\n                     </form>\r\n                     <div style={{ color: 'red' }}>\r\n                           {this.state.UsernameError}\r\n                        </div>\r\n\r\n                     <button className=\"login-button\" type=\"submit\" name=\"submit\" onClick={this.validate}>Login</button>\r\n                     <div>\r\n                        <h5>Not Have Account?<a href=\"/registration\">Register</a></h5>\r\n                     </div>\r\n                  </div>\r\n               </div>\r\n\r\n            </div>\r\n         )\r\n\r\n      }\r\n   }\r\n}\r\n","C:\\Users\\Ashish\\FreelanceMarket\\client\\src\\components\\Registration.js",["113","114"],"import React, { Component } from 'react'\r\nimport axios from 'axios';\r\nimport './componentCss/Form.css';\r\nexport default class Registration extends Component {\r\n\r\n\r\n    constructor(props) {\r\n        super(props);\r\n        this.state = {\r\n            username: \"\",\r\n            password: \"\",\r\n            fullname: \"\",\r\n            mobile: \"\",\r\n            email: \"\",\r\n            confirm: \"\",\r\n            Error: \"\"\r\n        };\